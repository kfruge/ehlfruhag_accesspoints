advwed <- function(obj, adv) {
tmon <- obj + 7*(adv-1)
twed <- tmon + 2
tmon <- format(tmon, format="%m/%d")
twed <- format(twed, format="%m/%d")
return(twed)
}
advthurs <- function(obj, adv) {
tmon <- obj + 7*(adv-1)
